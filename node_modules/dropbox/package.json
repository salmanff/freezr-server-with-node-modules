    "mocha": "^2.4.5",
    "pretty-bytes": "^2.0.1",
    "prompt": "^1.0.0",
    "sinon": "^1.17.3",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1"
  },
  "dependencies": {
